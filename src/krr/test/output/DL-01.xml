<KB>
    <EQUIVALENTTO>
        <CONCEPT>BlendedRedWine</CONCEPT>
        <AND>
            <CONCEPT>Wine</CONCEPT>
            <FILLS>
                <ROLE>Color</ROLE>
                <CONSTANT>red</CONSTANT>
            </FILLS>
            <EXISTS>
                <INTEGER>2</INTEGER>
                <ROLE>GrapeType</ROLE>
            </EXISTS>
        </AND>
    </EQUIVALENTTO>
    <EQUIVALENTTO>
        <CONCEPT>ProgressiveCompany</CONCEPT>
        <AND>
            <CONCEPT>Company</CONCEPT>
            <EXISTS>
                <INTEGER>7</INTEGER>
                <ROLE>Director</ROLE>
            </EXISTS>
            <ALL>
                <ROLE>Manager</ROLE>
                <AND>
                    <CONCEPT>Woman</CONCEPT>
                    <FILLS>
                        <ROLE>Degree</ROLE>
                        <CONSTANT>phD</CONSTANT>
                    </FILLS>
                </AND>
            </ALL>
            <FILLS>
                <ROLE>MinSalary</ROLE>
                <DIV>
                    <FLOAT>24.00</FLOAT>
                    <CONSTANT>hour</CONSTANT>
                </DIV>
            </FILLS>
        </AND>
    </EQUIVALENTTO>
</KB>
